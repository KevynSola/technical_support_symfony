{% extends 'base.html.twig' %}

{% block title %}Voir
{% endblock %}

{% block body %}
	<section>
		<div class="container">
			<div class="bloc-head d-flex justify-content-between align-items-center">
				<h1>Voir Auto</h1>
				<a class="btn btn-lg btn-primary m-3" href="{{ path('app_multiple_index') }}">Retour à l'index</a>
			</div>
			<table class="table">
				<tbody>
					<tr>
						<th>Id</th>
						<td>{{ multiple.id }}</td>
					</tr>
					<tr>
						<th>Nom</th>
						<td>{{ multiple.nom }}</td>
					</tr>
				</tbody>
			</table>


			<a class="btn btn-lg btn-warning m-3" href="{{ path('app_multiple_edit', {'id': multiple.id}) }}">Éditer</a>

			{{ include('multiple/_delete_form.html.twig') }}
		</div>
	</section>

{% endblock %}
